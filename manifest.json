{
  "manifest_version": 2,
  "name": "FIT Progtest auto shibboleth",
  "version": "1.1",
  "description": "Automatically choose shibboleth auth on progtest.",
  "content_scripts": [
    {
      "matches": [
        "*://progtest.fit.cvut.cz/*"
      ],
      "js": [
        "autoshib.js"
      ]
    }
  ],
  "icons": {
    "48": "icons/autoshib-48.png",
    "96": "icons/autoshib-96.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "fit-progtest-autoshib@saytam.cz"
    }
  }
}